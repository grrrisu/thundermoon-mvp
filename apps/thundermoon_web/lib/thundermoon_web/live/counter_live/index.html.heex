<h1>Counter</h1>

<div class="flex items-center mt-12">
  <div class="flex space-x-2 mr-16">
    <Digit.value id="digit-100" digit="100" value={@digits.digit_100} />
    <Digit.value id="digit-10" digit="10" value={@digits.digit_10} />
    <Digit.value id="digit-1" digit="1" value={@digits.digit_1} />
  </div>

  <ActionButtons.box current_user={@current_user} started={@started} />
</div>



