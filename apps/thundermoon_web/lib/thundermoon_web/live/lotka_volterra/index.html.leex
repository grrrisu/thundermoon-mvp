<h1>Lotka Volterra</h1>

<%= live_component @socket, ThundermoonWeb.LotkaVolterra.Chart, id: "chart-#{@sim_id}", vegetation: @vegetation %>

<div class="action-buttons">
  <%= start_button(@socket, "start-#{@sim_id}", @started) %>

  <%= live_redirect(to: Routes.lotka_volterra_new_path(@socket, :new), class: "button button-icon button-warning") do %>
    <i class="la la-2x la-reply-all"></i> recreate
  <% end %>
</div>
